frontend
- page_all__profile_classic|Frontend
    display
      check: url
      where: path
      match: %^.*$%
    styles
    - page_style
        path: frontend/page.cssd?page_id=%%_page_id_context
        weight: -500
        attributes
        - rel: stylesheet
        - media: all
    favicons
    # icon classic for browsers: Safari, Opera, Chrome, Edge …
    - icon_32_classic
        path: frontend/pictures/icon-favourite-32.ico
        attributes
        - rel: icon
        - type: image/x-icon
        - sizes: 32x32
    # icon for all modern browsers: Safari, Opera, Chrome, Firefox, Edge …
    - icon_32_modern
        path: frontend/pictures/icon-favourite-32.png
        attributes
        - rel: icon
        - type: image/png
        - sizes: 32x32
    # icon for Safari on iPad 1/2
    - icon_apple_76
        path: frontend/pictures/icon-favourite-76.png
        attributes
        - rel: apple-touch-icon
        - type: image/png
        - sizes: 76x76
    # icon for Safari on iPhone
    - icon_apple_120
        path: frontend/pictures/icon-favourite-120.png
        attributes
        - rel: apple-touch-icon
        - type: image/png
        - sizes: 120x120
    # icon for Safari on any iPad
    - icon_apple_152
        path: frontend/pictures/icon-favourite-152.png
        attributes
        - rel: apple-touch-icon
        - type: image/png
        - sizes: 152x152
    # icon for Safari on iPad 3+, iPhone, macOS
    - icon_apple_192
        path: frontend/pictures/icon-favourite-192.png
        attributes
        - rel: apple-touch-icon
        - type: image/png
        - sizes: 192x192